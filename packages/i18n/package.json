{
  "name": "@orderly.network/i18n",
  "version": "2.0.7",
  "description": "Internationalization for orderly sdk",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "converter": "bin/cli.js"
  },
  "scripts": {
    "build": "tsup && pnpm generateCsv",
    "converter": "node ./bin/cli.js",
    "generateCsv": "pnpm converter generateCsv ./dist/locale.csv",
    "csv2json": "pnpm converter csv2json ./dist/locale.csv ./dist/locale",
    "json2csv": "pnpm converter json2csv ./dist/locale/en.json,./dist/locale/zh.json ./dist/locale.csv",
    "diffcsv": "pnpm converter diffcsv ./dist/locale1.csv ./dist/locale2.csv",
    "fillJson": "pnpm converter fillJson ./src/locale/zh.json ./dist/locale/zh.json"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "react-i18next": "^15.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}