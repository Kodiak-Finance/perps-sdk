{
    "extends": "tsconfig/base.json",
    "compilerOptions": {
      "plugins": [{ "name": "next" }],
      "jsx": "preserve",
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules","__test__","__mocks__"],
    "typedocOptions": {
      "entryPoints": ["src/index.ts"],
